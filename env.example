
##############################################
# Snowflake Credentials
##############################################
# Example account format:
# xy12345.eu-west-2 (region-specific)
SNOWFLAKE_ACCOUNT=

# Username for authentication
SNOWFLAKE_USER=

# Password (if using password auth)
SNOWFLAKE_PASSWORD=

# Snowflake role with required privileges:
# e.g. SYSADMIN, TRANSFORMER, ANALYST
SNOWFLAKE_ROLE=

# Virtual warehouse for compute:
# e.g. COMPUTE_WH, TRANSFORM_WH
SNOWFLAKE_WAREHOUSE=

# Default database
SNOWFLAKE_DATABASE=

# Default schema
SNOWFLAKE_SCHEMA=


##############################################
# Optional: Private Key Authentication
##############################################
# Use instead of SNOWFLAKE_PASSWORD
# (Path to RSA private key file)
# SNOWFLAKE_PRIVATE_KEY_PATH=
# Passphrase for key (if applicable)
# SNOWFLAKE_PRIVATE_KEY_PASSPHRASE=


##############################################
# Optional: External Stage Credentials
##############################################
# Only needed if you're using S3, Azure Blob,
# or GCS external stages.
# For S3 staging:
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_SESSION_TOKEN=


##############################################
# Prefect (optional)
##############################################
PREFECT_API_URL=
PREFECT_API_KEY=
